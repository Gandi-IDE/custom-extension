const White_YSGJPro_picture = "data:image/svg+xml;base64,";

const White_YSGJPro_icon = "data:image/svg+xml;base64,";
const White_YSGJPro_extensionId = "WhiteYSGJ.Pro";

class ysgj {  
  constructor (runtime){
      this.runtime = runtime;
      this.step = this.runtime._step;
      this._formatMessage = runtime.getFormatMessage({
          "zh-cn": {
              "YSGJ.name": "White的更多工具Pro",
              "YSGJ.endl": "换行",
              "YSGJ.two1": "将十进制[TEXT]转换为二进制",
              "YSGJ.two2": "将二进制[TEXT2]转换为十进制",
              "YSGJ.keydown": "当按下[KEY]键",
              "YSGJ.inf": "无限",
              "YSGJ.int": "将小数[double]舍弃小数点以后数字",
              "YSGJ.int2": "将小数[double2]保留小数点后[double3]位",
              "YSGJ.power": "获取[num]的[power]次方",
              "YSGJ.power_2": "平方",
              "YSGJ.power_3": "立方",
          },
          en: {
              "YSGJ.name": "White More tool Pro",  
              "YSGJ.endl": "Line feed",
              "YSGJ.two1": "Converts decimal [TEXT] to binary",
              "YSGJ.two2": "Convert binary [TEXT2] to decimal",
              "YSGJ.keydown": "When the [KEY] key is pressed",
              "YSGJ.inf": "Infinity",
              "YSGJ.int": "Discard [double] after the decimal point",
              "YSGJ.int2": "Leave the decimal [double2] after the decimal point [double3]",
              "YSGJ.power_2": "²",
              "YSGJ.power_3": "³",
          }
      })
  }


    formatMessage(id) {
        return this._formatMessage({
            id,
            default: id,
            description: id
        });
    }

  getInfo() {
    return {
      id: White_YSGJPro_extensionId,
      name: this.formatMessage("YSGJ.name"),
      blockIconURI: White_YSGJPro_picture,
      menuIconURI: White_YSGJPro_icon,
      color1: "#0bdcf5",
      color2: "#ffffff",
      blocks: [
        "---" + "换行",
        {
          opcode: 'endl',
          blockType: Scratch.BlockType.REPORTER,
          text: this.formatMessage("YSGJ.endl")
        },
        {
          opcode: 'two1',
          blockType: Scratch.BlockType.REPORTER,
          text: this.formatMessage("YSGJ.two1"),
          arguments: {  
              TEXT: {  
                  type: Scratch.ArgumentType.STRING,  
                  defaultValue: 'G.unfight',
              }  
          }  
        },
        {
          opcode: 'two2',
          blockType: Scratch.BlockType.REPORTER,
          text: this.formatMessage("YSGJ.two2"),
          arguments: {  
              TEXT2: {  
                  type: Scratch.ArgumentType.NUMBER,  
                  defaultValue: '01000111 00101110 01110101 01101110 01100110 01101001 01100111 01101000 01110100',
              }  
          }  
        },
        {
          opcode: 'keydown',
          blockType: Scratch.BlockType.HAT,
          text: this.formatMessage("YSGJ.keydown"),
          arguments: {
              KEY: {
                  type: Scratch.ArgumentType.REPORTER,
                  defaultValue: '17',
                  menu: "keydown",
              }
          }
        },
        {
          opcode: 'inf',
          blockType: Scratch.BlockType.REPORTER,
          text: this.formatMessage("YSGJ.inf")
        },
        {
          opcode: 'int',
          blockType: Scratch.BlockType.REPORTER,
          text: this.formatMessage("YSGJ.int"),
          arguments: {
            double: {
              type: Scratch.ArgumentType.NUMBER,
              defaultValue: '114.514'
            }
          },
        },
        {
          opcode: 'int2',
          blockType: Scratch.BlockType.REPORTER,
          text: this.formatMessage("YSGJ.int2"),
          arguments: {
            double2: {
              type: Scratch.ArgumentType.NUMBER,
              defaultValue: '114.514'
            },
            double3: {
              type: Scratch.ArgumentType.NUMBER,
              defaultValue: '1'
            }
          },
        },
        {
          opcode: 'power',
          blockType: Scratch.BlockType.REPORTER,
          text: this.formatMessage("YSGJ.power"),
          arguments: {
            num: {
              type: Scratch.ArgumentType.NUMBER,
              defaultValue: '2'
            },
            power: {
              type: Scratch.ArgumentType.REPORTER,
              defaultValue: 'p_2',
              menu: "_power_",
            }
          },
        },
      ],
      menus: {
          keydown: [
              {
                text: "control",
                value: "17"
              },
              {
                text: "shift",
                value: "16"
              },
              {
                text: "caps lock",
                value: "20"
              },
              {
                text: "backspace",
                value: "8"
              },
              {
                text: "enter",
                value: "13"
              },
              {
                text: "alt",
                value: "18"
              },
              {
                text: "esc",
                value: "27"
              }
          ],
          _power_: [
            {
              text: this.formatMessage("YSGJ.power_2"),
              value: "1"
            },
            {
              text: this.formatMessage("YSGJ.power_3"),
              value: "2"
            }
        ],
      },
    };
  }

  endl() {
    return '\n';
  }
  two1(args) {
    const { TEXT } = args;
    let binaryString = '';
    for (let i = 0; i < TEXT.length; i++) {
    const asciiCode = TEXT.charCodeAt(i);
    const binaryChar = asciiCode.toString(2);
    binaryString += binaryChar.padStart(8, '0') + ' ';
    }
    return binaryString.trim();
  }
  two2(args) {
    const { TEXT2 } = args;
    const binaryArray = TEXT2.split(' ');
    let text = '';
    for (let i = 0; i < binaryArray.length; i++) {
    const binaryChar = binaryArray[i];
    const decimalChar = parseInt(binaryChar, 2);
    text += String.fromCharCode(decimalChar);
    }
    return text;
  }
  keydown(args) {
    document.addEventListener("keydown",keydown);
    function keydown(event){
        if(event.keyCode == value){
          true;
        }else{
          false;
        }
      }
  }
  inf() {
    return 1/0;
  }
  int(ages) {
    const { double } = ages;
    const int = double;
    let a = Math.trunc(int);
    return a;
  }
  int2(ages) {
    const { double2 } = ages;
    const { double3 } = ages;
    const int = double2;
    const int2 = double3;
    var r = (int).toFixed(int2)
    return r;
  }
  power(ages) {
    const { num } = ages;
    const { power } = ages;
    let a=value;
    if(power === 1)
    {
      a=num*num;
    }
    else
    {
      a=num*num*num;
    }
    return a;
  }
}
window.tempExt = {
    Extension: ysgj,
    info: {
        name: "YSGJ.name",
        description: "YSGJ.Description",
        extensionId: White_YSGJPro_extensionId,
        iconURL: White_YSGJPro_picture,
        insetIconURL: White_YSGJPro_icon,
        featured: true,
        disabled: false,
        collaborator: "White@CCW",
        collaboratorURL:'',
        collaboratorURL:'https://www.ccw.site/student/657922b91ddb475eb2208d4d',
    },
    l10n: {
        "zh-cn": {
            "YSGJ.name": "White的更多工具Pro",
            'YSGJ.Description' : '更多工具'
        },
        en: {
            "YSGJ.name": "White More tool Pro",
            'YSGJ.Description' : 'More tools'
        }
    },
};

Scratch.extensions.register(new ysgj());
